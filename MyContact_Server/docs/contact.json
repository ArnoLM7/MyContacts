{
	"/api/contact/all": {
		"get": {
			"tags": ["Contacts"],
			"summary": "Récupérer tous les contacts",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"responses": {
				"200": {
					"description": "Liste des contacts récupérée avec succès",
					"content": {
						"application/json": {
							"example": {
								"contacts": [
									{
										"_id": "64f4e5d19f5b9e6a8c2c1234",
										"firstName": "John",
										"lastName": "Doe",
										"phone": "0612345678"
									}
								],
								"success": true
							}
						}
					}
				},
				"500": {
					"description": "Erreur serveur",
					"content": {
						"application/json": {
							"example": {
								"success": false,
								"error": {
									"code": 500,
									"message": "Internal Server Error",
									"details": null
								}
							}
						}
					}
				}
			}
		}
	},
	"/api/contact/create": {
		"post": {
			"tags": ["Contacts"],
			"summary": "Créer un nouveau contact",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"requestBody": {
				"required": true,
				"content": {
					"application/json": {
						"example": {
							"firstName": "John",
							"lastName": "Doe",
							"phone": "0612345678"
						}
					}
				}
			},
			"responses": {
				"201": {
					"description": "Contact créé avec succès",
					"content": {
						"application/json": {
							"example": {
								"message": "Contact successfully created!",
								"result": {
									"_id": "64f4e5d19f5b9e6a8c2c1234",
									"firstName": "John",
									"lastName": "Doe",
									"phone": "0612345678"
								},
								"success": true
							}
						}
					}
				},
				"500": {
					"description": "Erreur serveur",
					"content": {
						"application/json": {
							"example": {
								"success": false,
								"error": {
									"code": 500,
									"message": "Internal Server Error",
									"details": null
								}
							}
						}
					}
				}
			}
		}
	},
	"/api/contact/update/{id}": {
		"patch": {
			"tags": ["Contacts"],
			"summary": "Mettre à jour un contact",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"parameters": [
				{
					"name": "id",
					"in": "path",
					"required": true,
					"schema": {
						"type": "string"
					},
					"description": "ID du contact à mettre à jour"
				}
			],
			"requestBody": {
				"required": true,
				"content": {
					"application/json": {
						"example": {
							"firstName": "Johnny",
							"lastName": "Doe",
							"phone": "0611111111"
						}
					}
				}
			},
			"responses": {
				"200": {
					"description": "Contact mis à jour avec succès",
					"content": {
						"application/json": {
							"example": {
								"message": "Contact successfully updated!",
								"result": {
									"_id": "64f4e5d19f5b9e6a8c2c1234",
									"firstName": "Johnny",
									"lastName": "Doe",
									"phone": "0611111111"
								},
								"success": true
							}
						}
					}
				},
				"404": {
					"description": "Contact non trouvé",
					"content": {
						"application/json": {
							"example": {
								"success": false,
								"error": {
									"code": 404,
									"message": "Contact not found",
									"details": null
								}
							}
						}
					}
				},
				"500": {
					"description": "Erreur serveur",
					"content": {
						"application/json": {
							"example": {
								"success": false,
								"error": {
									"code": 500,
									"message": "Internal Server Error",
									"details": null
								}
							}
						}
					}
				}
			}
		}
	},
	"/api/contact/delete/{id}": {
		"delete": {
			"tags": ["Contacts"],
			"summary": "Supprimer un contact",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"parameters": [
				{
					"name": "id",
					"in": "path",
					"required": true,
					"schema": {
						"type": "string"
					},
					"description": "ID du contact à supprimer"
				}
			],
			"responses": {
				"200": {
					"description": "Contact supprimé avec succès",
					"content": {
						"application/json": {
							"example": {
								"message": "Contact successfully deleted!",
								"result": {
									"_id": "64f4e5d19f5b9e6a8c2c1234",
									"firstName": "John",
									"lastName": "Doe",
									"phone": "0612345678"
								},
								"success": true
							}
						}
					}
				},
				"404": {
					"description": "Contact non trouvé",
					"content": {
						"application/json": {
							"example": {
								"success": false,
								"error": {
									"code": 404,
									"message": "Contact not found",
									"details": null
								}
							}
						}
					}
				},
				"500": {
					"description": "Erreur serveur",
					"content": {
						"application/json": {
							"example": {
								"success": false,
								"error": {
									"code": 500,
									"message": "Internal Server Error",
									"details": null
								}
							}
						}
					}
				}
			}
		}
	},
	"components": {
		"securitySchemes": {
			"bearerAuth": {
				"type": "http",
				"scheme": "bearer",
				"bearerFormat": "JWT"
			}
		}
	}
}
